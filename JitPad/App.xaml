                <internals:SharedResourceDictionary Source="pack://application:,,,/Biaui;component/StandardControls/ContextMenu.xaml" />
                <internals:SharedResourceDictionary Source="pack://application:,,,/Biaui;component/StandardControls/Decorator.xaml" />
